<div class="form-group">
  <label for="nome" class="col-sm-2 control-label">Nome</label>
  <div class="col-sm-10">
    <input type="text" class="form-control" id="nome" name="nome" placeholder="Nome" value="<%= paciente.nome %>" required>
  </div>
</div>

<div class="form-group">
  <label for="email" class="col-sm-2 control-label">Email</label>
  <div class="col-sm-10">
    <input type="email" class="form-control" id="email" name="email" placeholder="Email" value="<%= paciente.email %>" required>
  </div>
</div>

<div class="form-group">
  <label for="telefone" class="col-sm-2 control-label">Telefone</label>
  <div class="col-sm-10">
    <input type="tel" class="form-control" id="telefone" name="telefone" placeholder="(61) 9999-9999" value="<%= paciente.telefone %>" required>
  </div>
</div>